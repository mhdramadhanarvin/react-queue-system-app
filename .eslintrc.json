{
  "env": {
      "node": false,
      "commonjs": true,
      "es2021": true,
      "jest": true 
  },
  "extends": "eslint:recommended",
  "overrides": [
  ], 
  "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
  },
  "rules": {
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "double"
      ],
      "semi": [
          "error",
          "never"
      ]
  }
}
